<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Employee Registration App</display-name>
	<servlet>
		<servlet-class>com.servlet.jdbc_crud_with_servlet_api.client.controller.InsertEmployeeController</servlet-class>
		<servlet-name>register</servlet-name>
	</servlet>

	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/employRegister</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-class>com.servlet.jdbc_crud_with_servlet_api.client.controller.LoginEmployController</servlet-class>
		<servlet-name>login</servlet-name>
	</servlet>

	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/empLogin</url-pattern>
	</servlet-mapping>
</web-app>
